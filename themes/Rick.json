{
  "name": "Rick Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#121212",
    "editor.foreground": "#e0e0e0",
    "sideBar.background": "#1a1a1a",
    "sideBar.foreground": "#d0d0d0",
    "activityBar.background": "#1a1a1a",
    "activityBar.foreground": "#f0f0f0",
    "statusBar.background": "#1a1a1a",
    "statusBar.foreground": "#e0e0e0",
    "titleBar.activeBackground": "#1a1a1a",
    "panel.background": "#1a1a1a",
    "tab.inactiveBackground": "#181818",
    "tab.activeBackground": "#202020",
    "editor.selectionBackground": "#282828",
    "editor.selectionHighlightBackground": "#30303055",
    "editor.inactiveSelectionBackground": "#24242455",
    "editorLineNumber.foreground": "#5a5a5a",
    "editorLineNumber.activeForeground": "#9a9a9a",
    "editorBracketMatch.background": "#22334433",
    "editorBracketMatch.border": "#3b82f6",
    "editorWhitespace.foreground": "#2a2a2a",
    "minimap.background": "#121212",
    "panel.border": "#222222",
    "input.background": "#1c1c1c",
    "activityBarBadge.background": "#3b82f6",
    "activityBarBadge.foreground": "#ffffff",
    "badge.background": "#3b82f6",
    "badge.foreground": "#ffffff",
    "gitDecoration.modifiedResourceForeground": "#60a5fa",
    "gitDecoration.addedResourceForeground": "#93c5fd",
    "gitDecoration.deletedResourceForeground": "#2563eb",
    "scrollbarSlider.background": "#30303088",
    "scrollbarSlider.hoverBackground": "#40404088",
    "scrollbarSlider.activeBackground": "#505050aa"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "comment.block.documentation"],
      "settings": { "foreground": "#7a7a7a", "fontStyle": "italic" }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator"
      ],
      "settings": { "foreground": "#3b82f6", "fontStyle": "bold" }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "variable.function"
      ],
      "settings": { "foreground": "#60a5fa" }
    },
    {
      "scope": ["entity.name.method", "support.function.method", "variable.function.constructor"],
      "settings": { "foreground": "#60a5fa" }
    },
    {
      "scope": ["variable", "variable.other", "support.variable", "entity.name.variable"],
      "settings": { "foreground": "#e0e0e0" }
    },
    {
      "scope": ["variable.parameter", "variable.parameter.function", "variable.other.readwrite"],
      "settings": { "foreground": "#d0d0d0" }
    },
    {
      "scope": ["string", "string.quoted", "string.template", "string.interpolated"],
      "settings": { "foreground": "#93c5fd" }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "storage.type.class",
        "entity.name.class",
        "support.class"
      ],
      "settings": { "foreground": "#e0e0e0" }
    },
    {
      "scope": ["tag", "entity.name.tag", "markup.heading", "entity.name.section"],
      "settings": { "foreground": "#3b82f6", "fontStyle": "bold" }
    },
    {
      "scope": ["attribute.name", "entity.other.attribute-name.html"],
      "settings": { "foreground": "#ffd700" }
    },
    {
      "scope": ["support.constant.property-value.css", "support.type.property-name.css"],
      "settings": { "foreground": "#7fffd4" }
    },
    {
      "scope": [
        "punctuation.definition.string.begin.html",
        "punctuation.definition.string.end.html"
      ],
      "settings": { "foreground": "#93c5fd" }
    }
  ]
}
