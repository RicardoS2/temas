{
  "name": "Rick Blue",
  "type": "dark",
  "colors": {
    "editor.background": "#111111",
    "editor.foreground": "#dcdcdc",
    "sideBar.background": "#111111",
    "sideBar.foreground": "#cfcfcf",
    "activityBar.background": "#111111",
    "activityBar.foreground": "#e6e6e6",
    "statusBar.background": "#111111",
    "statusBar.foreground": "#dcdcdc",
    "titleBar.activeBackground": "#111111",
    "panel.background": "#111111",
    "tab.inactiveBackground": "#111111",
    "tab.activeBackground": "#151515",
    "editor.selectionBackground": "#181818",
    "editor.selectionHighlightBackground": "#1c1c1c00",
    "editor.inactiveSelectionBackground": "#14141400",
    "editorLineNumber.foreground": "#3f3f3f",
    "editorLineNumber.activeForeground": "#7f7f7f",
    "editorIndentGuide.activeBackground1": "#151515",
    "editorBracketMatch.background": "#22000022",
    "editorBracketMatch.border": "#3b82f6",
    "editorWhitespace.foreground": "#222222",
    "minimap.background": "#111111",
    "panel.border": "#1b1b1b",
    "input.background": "#141414",
    "activityBarBadge.background": "#3b82f6",
    "activityBarBadge.foreground": "#ffffff",
    "badge.background": "#3b82f6",
    "badge.foreground": "#ffffff",
    "gitDecoration.modifiedResourceForeground": "#60a5fa",
    "gitDecoration.addedResourceForeground": "#93c5fd",
    "gitDecoration.deletedResourceForeground": "#2563eb",
    "scrollbarSlider.background": "#20202088",
    "scrollbarSlider.hoverBackground": "#30303088",
    "scrollbarSlider.activeBackground": "#404040aa"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "comment.block.documentation"],
      "settings": { "foreground": "#646464", "fontStyle": "italic" }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator"
      ],
      "settings": { "foreground": "#3b82f6", "fontStyle": "bold" }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "variable.function"
      ],
      "settings": { "foreground": "#60a5fa" }
    },
    {
      "scope": ["entity.name.method", "support.function.method", "variable.function.constructor"],
      "settings": { "foreground": "#60a5fa" }
    },
    {
      "scope": ["variable", "variable.other", "support.variable", "entity.name.variable"],
      "settings": { "foreground": "#dcdcdc" }
    },
    {
      "scope": ["variable.parameter", "variable.parameter.function", "variable.other.readwrite"],
      "settings": { "foreground": "#cfcfcf" }
    },
    {
      "scope": ["string", "string.quoted", "string.template", "string.interpolated"],
      "settings": { "foreground": "#93c5fd" }
    }
  ]
}
