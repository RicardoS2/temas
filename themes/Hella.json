{
  "name": "Hella Theme",
  "type": "dark",
  "colors": {
    "editor.background": "#111111",
    "editor.foreground": "#e0e0e0",
    "sideBar.background": "#121212",
    "sideBar.foreground": "#cfcfcf",
    "activityBar.background": "#121212",
    "activityBar.foreground": "#f0f0f0",
    "statusBar.background": "#121212",
    "statusBar.foreground": "#e0e0e0",
    "titleBar.activeBackground": "#121212",
    "panel.background": "#121212",
    "tab.inactiveBackground": "#141414",
    "tab.activeBackground": "#1a1a1a",
    "editor.selectionBackground": "#1f1f1f",
    "editor.selectionHighlightBackground": "#22222255",
    "editor.inactiveSelectionBackground": "#19191955",
    "editorLineNumber.foreground": "#4a4a4a",
    "editorLineNumber.activeForeground": "#8a8a8a",
    "editorBracketMatch.background": "#22000033",
    "editorBracketMatch.border": "#ff5555",
    "editorWhitespace.foreground": "#1c1c1c",
    "minimap.background": "#111111",
    "panel.border": "#222222",
    "input.background": "#141414",
    "activityBarBadge.background": "#ff4444",
    "activityBarBadge.foreground": "#ffffff",
    "badge.background": "#ff4444",
    "badge.foreground": "#ffffff",
    "gitDecoration.modifiedResourceForeground": "#ff9966",
    "gitDecoration.addedResourceForeground": "#66ff99",
    "gitDecoration.deletedResourceForeground": "#ff6666",
    "scrollbarSlider.background": "#30303088",
    "scrollbarSlider.hoverBackground": "#40404088",
    "scrollbarSlider.activeBackground": "#505050aa"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "comment.block.documentation"],
      "settings": { "foreground": "#7a7a7a", "fontStyle": "italic" }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator"
      ],
      "settings": { "foreground": "#ff6666", "fontStyle": "bold" }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "variable.function"
      ],
      "settings": { "foreground": "#ff8844" }
    },
    {
      "scope": ["entity.name.method", "support.function.method", "variable.function.constructor"],
      "settings": { "foreground": "#ff8844" }
    },
    {
      "scope": ["variable", "variable.other", "support.variable", "entity.name.variable"],
      "settings": { "foreground": "#e0e0e0" }
    },
    {
      "scope": ["variable.parameter", "variable.parameter.function", "variable.other.readwrite"],
      "settings": { "foreground": "#cfcfcf" }
    },
    {
      "scope": ["string", "string.quoted", "string.template", "string.interpolated"],
      "settings": { "foreground": "#ff9966" }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.numeric.float",
        "constant.numeric.hex"
      ],
      "settings": { "foreground": "#ff7777" }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "storage.type.class",
        "entity.name.class",
        "support.class"
      ],
      "settings": { "foreground": "#e0e0e0" }
    },
    {
      "scope": ["tag", "entity.name.tag", "markup.heading", "entity.name.section"],
      "settings": { "foreground": "#ff4444", "fontStyle": "bold" }
    },
    {
      "scope": [
        "attribute.name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx"
      ],
      "settings": { "foreground": "#ffd700" }
    },
    {
      "scope": ["support.constant.property-value.css", "support.type.property-name.css"],
      "settings": { "foreground": "#7fffd4" }
    },
    {
      "scope": [
        "punctuation.definition.string.begin.html",
        "punctuation.definition.string.end.html",
        "punctuation.definition.string.begin.jsx",
        "punctuation.definition.string.end.jsx"
      ],
      "settings": { "foreground": "#ff9966" }
    },
    {
      "scope": ["keyword.operator.jsx", "meta.jsx.children"],
      "settings": { "foreground": "#ff8844" }
    },
    {
      "scope": [
        "support.function.builtin.css",
        "support.constant.property-value.css",
        "support.type.property-name.css"
      ],
      "settings": { "foreground": "#7fffd4" }
    }
  ]
}
