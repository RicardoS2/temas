{
  "name": "Hella Theme Refined",
  "type": "dark",
  "colors": {
    "editor.background": "#111111",
    "editor.foreground": "#e4e4e4",
    "sideBar.background": "#141414",
    "sideBar.foreground": "#cccccc",
    "activityBar.background": "#141414",
    "activityBar.foreground": "#f5f5f5",
    "statusBar.background": "#141414",
    "statusBar.foreground": "#e4e4e4",
    "titleBar.activeBackground": "#141414",
    "panel.background": "#141414",
    "tab.inactiveBackground": "#181818",
    "tab.activeBackground": "#1f1f1f",
    "editor.selectionBackground": "#1f1f1f",
    "editor.selectionHighlightBackground": "#25252555",
    "editor.inactiveSelectionBackground": "#1a1a1a55",
    "editorLineNumber.foreground": "#4d4d4d",
    "editorLineNumber.activeForeground": "#a3a3a3",
    "editorBracketMatch.background": "#33000033",
    "editorBracketMatch.border": "#ff4d4d",
    "editorWhitespace.foreground": "#1a1a1a",
    "minimap.background": "#111111",
    "panel.border": "#1c1c1c",
    "input.background": "#181818",
    "activityBarBadge.background": "#ff4d4d",
    "activityBarBadge.foreground": "#ffffff",
    "badge.background": "#ff4d4d",
    "badge.foreground": "#ffffff",
    "gitDecoration.modifiedResourceForeground": "#ff9966",
    "gitDecoration.addedResourceForeground": "#66ff99",
    "gitDecoration.deletedResourceForeground": "#ff6666",
    "scrollbarSlider.background": "#2a2a2a88",
    "scrollbarSlider.hoverBackground": "#3a3a3a88",
    "scrollbarSlider.activeBackground": "#4a4a4aaa"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "comment.block.documentation"],
      "settings": { "foreground": "#777777", "fontStyle": "italic" }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier",
        "keyword.control",
        "keyword.operator"
      ],
      "settings": { "foreground": "#ff4d4d", "fontStyle": "bold" }
    },
    {
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "support.function",
        "variable.function"
      ],
      "settings": { "foreground": "#ff704d" }
    },
    {
      "scope": ["entity.name.method", "support.function.method", "variable.function.constructor"],
      "settings": { "foreground": "#ff704d" }
    },
    {
      "scope": ["variable", "variable.other", "support.variable", "entity.name.variable"],
      "settings": { "foreground": "#e4e4e4" }
    },
    {
      "scope": ["variable.parameter", "variable.parameter.function", "variable.other.readwrite"],
      "settings": { "foreground": "#cccccc" }
    },
    {
      "scope": ["string", "string.quoted", "string.template", "string.interpolated"],
      "settings": { "foreground": "#ff9966" }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language",
        "constant.numeric.float",
        "constant.numeric.hex"
      ],
      "settings": { "foreground": "#ff8080" }
    },
    {
      "scope": [
        "entity.name.type",
        "support.type",
        "storage.type.class",
        "entity.name.class",
        "support.class"
      ],
      "settings": { "foreground": "#e4e4e4" }
    },
    {
      "scope": ["tag", "entity.name.tag", "markup.heading", "entity.name.section"],
      "settings": { "foreground": "#ff4d4d", "fontStyle": "bold" }
    },
    {
      "scope": [
        "attribute.name",
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx"
      ],
      "settings": { "foreground": "#ffd966" }
    },
    {
      "scope": ["support.constant.property-value.css", "support.type.property-name.css"],
      "settings": { "foreground": "#99e2b4" }
    },
    {
      "scope": [
        "punctuation.definition.string.begin.html",
        "punctuation.definition.string.end.html",
        "punctuation.definition.string.begin.jsx",
        "punctuation.definition.string.end.jsx"
      ],
      "settings": { "foreground": "#ff9966" }
    },
    {
      "scope": ["keyword.operator.jsx", "meta.jsx.children"],
      "settings": { "foreground": "#ff704d" }
    },
    {
      "scope": [
        "support.function.builtin.css",
        "support.constant.property-value.css",
        "support.type.property-name.css"
      ],
      "settings": { "foreground": "#99e2b4" }
    }
  ]
}
